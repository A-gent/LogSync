<Window x:Class="LogSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogSync"
        xmlns:views = "clr-namespace:LogSync.View"
        mc:Ignorable="d"
        Title="LogSync">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Load Logs..." Click="Open_Click"></MenuItem>
            <!--<MenuItem Header="Sync Logs" Click="Sync_Click"></MenuItem>-->
        </Menu>
        <TextBox AcceptsReturn="True" />
        <Grid Margin="5" DockPanel.Dock="Top">
            <Grid Name="logGrid" Background="AliceBlue">
            </Grid>
        </Grid>
    </DockPanel>
</Window>
